{
  "name": "gibolt",
  "version": "1.0.0",
  "description": "Test of react 4th edition",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/lib/frontend/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|sass)$": "<rootDir>/lib/tests/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/lib/tests/setup.js"
    ]
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "private": true,
  "scripts": {
    "serve": "NODE_ENV=production node dist/server.js",
    "serve-server": "WEBPACK_ENV=server NODE_ENV=development webpack",
    "serve-client": "WEBPACK_ENV=browser NODE_ENV=development webpack-dev-server",
    "build-client": "WEBPACK_ENV=browser NODE_ENV=production webpack",
    "build-server": "WEBPACK_ENV=server NODE_ENV=production webpack",
    "lint": "eslint --cache --ext .jsx --ext .js lib/",
    "fix": "prettier --write '{,lib/tests/**/,lib/frontend/src/**/}*.js?(x)'",
    "test": "node ./node_modules/jest-cli/bin/jest.js --no-cache",
    "test-debug": "inspect ./node_modules/jest-cli/bin/jest.js --runInBand --env jest-environment-node-debug",
    "lint-all": "make fix lint"
  },
  "pre-commit": "lint-all",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bem-cn": "2.1.3",
    "date-fns": "2.0.0-alpha.7",
    "deep-equal": "1.0.1",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "koaze": "0.0.11",
    "path-to-regexp": "2.1.0",
    "query-string": "5.0.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "3.1.0",
    "react-notification-system-redux": "1.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redbox-react": "1.5.0",
    "redux": "3.7.2",
    "redux-http-status": "^0.0.7",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.12.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "1.1.5",
    "ignore-loader": "0.1.2",
    "import-sort-style-module": "3.0.0",
    "jest": "21.2.1",
    "jest-environment-node-debug": "2.0.0",
    "json-loader": "0.5.7",
    "node-sass": "4.7.2",
    "pre-commit": "1.2.2",
    "prettier": "1.8.2",
    "react-test-renderer": "16.2.0",
    "resolve-url-loader": "2.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.9.1",
    "webpack-dev-server": "2.9.5",
    "webpackozea": "0.0.9"
  }
}
